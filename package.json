{
  "name": "rag-llm-news-article-agent",
  "version": "1.0.0",
  "description": "RAG/LLL News Article Agent",
  "license": "ISC",
  "author": "Volodymyr Bordiian",
  "main": "server.js",
  "scripts": {
    "lint": "eslint src/**/*.ts tests/**/*/*.test.ts",
    "format": "eslint src/**/*.ts tests/**/*/*.test.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\" \"tests/**/*/*.test.ts\"",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "dev:windows": "concurrently \"npx tsc --watch\" \"nodemon --legacy-watch -q dist/server.js\"",
    "test": "jest"
  },
  "dependencies": {
    "cli-color": "^2.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
